<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SistemaLicenca.Views.Register"
             Title="Register">
    <Shell.BackButtonBehavior>
        <BackButtonBehavior IsVisible="False" IsEnabled="False" />
    </Shell.BackButtonBehavior>
    <VerticalStackLayout>
        <Frame Margin="50,50,50,50">
            <StackLayout>
                <Label Text="Usuario"/>
                <Entry x:Name="txt_user"/>
                <Label Text="Senha"/>
                <Entry x:Name="txt_pass"/>
                <Label Text="Ativação"/>
                <Entry x:Name="txt_activation"/>
                <Button Margin="0,10,0,10" x:Name="btnRegister" Clicked="btnRegister_Clicked" Text="Registrar"/>
                <Button x:Name="btnVoltar" Clicked="btnVoltar_Clicked" Text="Voltar"/>
            </StackLayout>
        </Frame>
    </VerticalStackLayout>
</ContentPage>